<%= render 'layouts/navbar' %>
<div id="tours">
  <div class='container'>
    <h2>
      Onde vocÃª gostaria de ir?
    </h2>
    <small class="text-gray-dark" style="display:block; text-align: center"><%= t('explore-tagline') %></small>
    <form action="#" class="out-of-couch">
      <input id="places-input-action" placeholder="<%= t('intro-location-label') %>" type="text">
      <button class='button' type='submit'>
        <%= t('where-to-go-text') %>
      </button>
    </form>
  </div>
  <div id="map_canvas"></div>
  <% if @backgrounds.present? %>
  <h4 style="margin-left: 40px;"><%= t('welcome-places-title') %></h4>
  <ul>
    <% @backgrounds.each do |b| %>
    <li>
      <%= link_to where_path(b.wheres.first) do %>
        <div>
          <h3><%= b.name %></h3>
          <h4><%= b.wheres.first.state %></h4>
        </div>
        <%= image_tag b.picture.url(:medium) %>
      <% end %>
    </li>
    <% end %>
  </ul>
  <% else %>
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            <div class="card card-block text-xs-center">
              <img class="card-img-top" src="https://unsplash.it/g/700/200/?gravity=east" class="img-fluid">
              <div class="card-block">
                <h4 class="card-title"><%= t('no-places-title') %></h4>
                <p class="card-text"><%= t('no-places-text') %></p>
                <a href="<%= organizer_welcome_path %>" class="btn btn-primary"><%= t('no-places-action') %></a>
              </div>
            </div>
          </div>
        </div>
      </div>
  <% end %>
</div>
